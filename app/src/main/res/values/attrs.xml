<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PartitionView">
        <attr name="leftWidth" format="dimension" />
        <attr name="centerStartX" format="dimension" />
        <attr name="centerStartY" format="dimension" />
        <attr name="centerWidth" format="dimension" />
        <attr name="centerHeight" format="dimension" />
        <attr name="lineColor" format="color" />
        <attr name="lineType" format="enum">
            <enum name="dashed" value="0" />
            <enum name="solid" value="1" />
        </attr>
    </declare-styleable>

</resources>